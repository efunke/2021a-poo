public class Ex_a06_10{
public static void main(String args[]){
for (int i = 0; i<16;i+=5){
  if ((i%5) == 0) {
    if (i == 0){
        System.out.println("O valor da variavel contador é : " +1);
    }else{
           System.out.println("O valor da variavel contador é i: " +i);
          }//Else
    }//i ==0
 }//For
}//Main
}//Programa


